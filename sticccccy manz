#include <stdio.h>
 
int main(void)
{
    int i;
    printf("(");
    for(i = 0; i<5;++i){
        printf("*");
    }
    printf(")");
    
    for(i = 0; i < 2; ++i) {
        printf("\n   ||");
    }
    printf("\n|");
    for(i = 0;i < 6; ++i) {
        printf("-");
    }
    printf("|"); 
    printf("\n|  ||  |");
    for(i = 0; i<5;++i){
        printf("\n   ||");
    }
    
    printf("\n");
    for(i = 0; i<2; ++i){
        printf("-");
    }
    printf("|  |");
    for(i = 0; i<2; ++i){
        printf("-");
    }
    return 0;
}
